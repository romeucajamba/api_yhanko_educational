
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Users {
  id String @id @default(uuid())
  name String
  email String
  password String
  bornDate String
  expertise  AreaOfExpertise
  country  Country
  gender   Gender


  @@map("users")
}

enum AreaOfExpertise {
  FRONTEND_DEVELOPER
  BACKEND_DEVELOPER
  FULLSTACK_DEVELOPER
  UI_UX_DESIGNER
  GRAPHIC_DESIGNER
  DATA_SCIENTIST
  DEVOPS
  MOBILE_DEVELOPER
  SECURITY_ENGINEER
  CLOUD_ENGINEER
  AI_ML_ENGINEER
  GAME_DEVELOPER
